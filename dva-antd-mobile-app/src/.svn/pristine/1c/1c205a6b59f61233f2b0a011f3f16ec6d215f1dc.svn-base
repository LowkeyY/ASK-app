@PrefixCls : selectmenu;
:global {
	.@{PrefixCls}{
		position: absolute;
		top: 0;
		right:0;
		height: 100%;
		width: 100%;
		&-menu{
			position: absolute;
			z-index: 70 !important;
			width: 100%;
		}
		&-mask{
			position: absolute;
			top: 0;
			width: 100%;
			height: 100%;
			background-color: #000;
			opacity: 0.4;
			z-index: 69;
		}
	}
}