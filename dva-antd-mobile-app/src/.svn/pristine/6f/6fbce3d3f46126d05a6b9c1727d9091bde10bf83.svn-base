@PrefixCls: fontsize;
.@{PrefixCls}-silder-box{
  width: 100%;
  height:100px;
  position: fixed;
  bottom: 0;
  left:0;

}
.@{PrefixCls}-font-small{
  font-size: 32px;
  position: absolute;
  margin: 0;
  top: 30px;
  left:55px;
  z-index: 2;
}
.@{PrefixCls}-font-big{
  font-size:.5rem;
  position: absolute;
  top: 18px;
  right:25px;
  z-index: 2;
  margin: 0;
}
:global{
  .am-slider-wrapper{
    height:35px;
    .am-slider>.am-slider-rail{
      background: #888;
    }
  }

}
.@{PrefixCls}-scale-box{
  padding:0  92px;
}
.@{PrefixCls}-scale{
  position: relative;
  width:100%;
  z-index: 2;
   &-1,&-2,&-3,&-4,&-5{
     width: 0.04rem;
    height:30px;
    background: #888;
     position: absolute;
     top: 20px;
  }
  &-1{
    left: 0;

  }
  &-2{
    left: 25%;

  }
  &-3{
    left: 50%;

  }
  &-4{
    left: 75%;

  }
  &-5{
    left: 100%;

  }
}
